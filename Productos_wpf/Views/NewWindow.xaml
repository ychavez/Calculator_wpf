<Window x:Class="Productos_wpf.Views.NewWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Productos_wpf.Views"
        mc:Ignorable="d"
        Title="NewWindow" Height="450" Width="800">
    <Grid>

        <StackPanel>
            <TextBox x:Name="txtNombre"/>
            <TextBox x:Name="txtCategoria"/>
            <TextBox x:Name="txtDescripcion"/>
            <TextBox x:Name="txtPrecio"/>
            <Button x:Name="btnAceptar" Content="Aceptar" Click="btnAceptar_Click"/>
        </StackPanel>
    </Grid>
</Window>
